{
  "rules": {
      "housemates": {
          "$housemateid": {
              "name": {
                  ".validate": "newData.isString()"
              },
              "username": {
                  ".validate": "newData.isString()"
              },
              "email": {},
              "houses": {},
              "$other": {".validate": false}
          }
      },
      "houses": {
          "$housecode": {
              "name": {},
              "members": {},
              "grocerylist": {},
              "receipts": {},
              "$other": {".validate": false}
          }
      },
      "grocerylists": {
          "$grocerylistid": {
              "name": {},
              "groceryitems": {
                  "$groceryitemid": {
                      ".validate": "newData.hasChildren(['name', 'quantity'])",
                      "name": {".validate": true},
                      "quantity": {".validate": true},
                      "$other": {".validate": false}
                  }
              },
              "$other": {".validate": false}
          }
      },
      "receipts": {
          "$receiptid": {
              "name": {},
              "groceryitems": {
                  "$groceryitemid": {
                      ".validate": "newData.hasChildren(['name', 'quantity', 'price'])",
                      "name": {".validate": true},
                      "quantity": {".validate": true},
                      "price": {".validate": true},
                      "$other": {".validate": false}
                  }
              },
              "image": {},
              "$other": {".validate": false}
          }
      }
  }
}